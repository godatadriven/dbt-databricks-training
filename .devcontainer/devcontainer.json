{
  "name": "dbt Databricks Codespace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceFolder": "/workspaces/dbt-databricks-training",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "innoverio.vscode-dbt-power-user"
      ],
      "settings": {}
    }
  },
  "remoteUser": "vscode",
  "remoteEnv": {
    "DBT_PROFILES_DIR": "/workspaces/dbt-databricks-training",
    "DBT_DATABRICKS_TOKEN": "${DBT_DATABRICKS_TOKEN}",
    "DBT_DATABRICKS_HOST": "${DBT_DATABRICKS_HOST}",
    "DBT_DATABRICKS_HTTP_PATH": "${DBT_DATABRICKS_HTTP_PATH}"
  }
}
